@PhdThesis{Bell2004,
  author   = {Bell, Andrea Moreira},
  school   = {Oregon State University},
  title    = {Hilbert modular surfaces and uniformizing groups of {K}lein invariants},
  year     = {2004},
  address  = {Ann Arbor},
  abstract = {In 1976, F. Hirzebruch [9] showed that the smooth model Y 2 of the Hilbert modular surface of level 2 for the field Q ([special characters omitted]) is isomorphic to the Klein surface of the icosahedron obtained from a blowing-up of [special characters omitted]. Later, T. Schmidt [16] showed that under this isomorphism, the Cohen-Wolfart embedding of the non-arithmetic Hecke group of signature (2, 5, ∞) has a lift in Y2 that corresponds to six specific exceptional divisors of the Klein surface.    In this dissertation, we consider the Klein A5 -Invariants of [special characters omitted] as seen in the Klein surface. We show, using Hirzebruch's isomorphism, that their images in the Hilbert modular surface X of level one are curves uniformized by non-compact and non-arithmetic triangle groups contained in the Hilbert modular group. We also give a correspondence between the A5-orbits of [special characters omitted] and the elliptic singularities of the surface  X.},
  comment  = {Copyright - Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.
