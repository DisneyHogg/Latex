  journal  = {Nuclear Physics B},
  title    = {Closed 2-dyon orbits},
  year     = {1989},
  issn     = {0550-3213},
  number   = {2},
  pages    = {447--472},
  volume   = {313},
  abstract = {This work is a study of the classical interaction and behavior of two slowly moving, electrically charged SU(2) magnetic monopoles in a space free of other particles. We determine that, in general, the dynamical system describing two-SU(2)-monopole motion is non-integrable. Thus, the monopole dynamical system permits an interpretation as a near-integrable hamiltonian dynamical system. We use secular perturbation theory to conclude that closed dyon orbits exist, and write a computer program to find some of them. We also survey the types of motions one might expect in R3.},
  doi      = {10.1016/0550-3213(89)90328-3},
  url      = {https://www.sciencedirect.com/science/article/pii/0550321389903283},
}

